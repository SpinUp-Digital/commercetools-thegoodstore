{
  "name": "thegoodstore-data",
  "scripts": {
    "build": "rimraf dist/ && babel ./lib/*.js --out-dir dist/lib/ --ignore ./node_modules,./.babelrc,./package.json --copy-files",
    "format": "prettier --write ./lib/**",
    "reset:data": "node dist/lib/data.js --start",
    "import:data": "node dist/lib/data.js --all:import",
    "import:projectSetup": "node dist/lib/project-setup.js --import",
    "import:zones": "node dist/lib/zones.js --import",
    "import:taxCategories": "node dist/lib/tax-categories.js --import",
    "import:channels": "node dist/lib/channels.js --import",
    "import:stores": "node dist/lib/stores.js --import",
    "import:categories": "node dist/lib/categories.js --import",
    "import:shippingmethods": "node dist/lib/shipping-method.js --import",
    "import:productType": "node dist/lib/products.js --importtypes",
    "import:products": "node dist/lib/products.js --import",
    "import:inventory": "node dist/lib/inventory.js --import",
    "import:customers": "node dist/lib/customers.js --import",
    "import:orders": "node dist/lib/orders.js --import",
    "import:types": "node dist/lib/types.js --import",
    "clean:data": "node dist/lib/data.js --all:clean",
    "clean:stores": "node dist/lib/stores.js --clean",
    "clean:categories": "node dist/lib/categories.js --clean",
    "clean:inventory": "node dist/lib/inventory.js --clean",
    "clean:channels": "node dist/lib/channels.js --clean",
    "clean:orders": "node dist/lib/orders.js --clean",
    "clean:carts": "node dist/lib/carts.js --clean",
    "clean:customers": "node dist/lib/customers.js --clean",
    "clean:shippingmethods": "node dist/lib/shipping-method.js --clean",
    "clean:zones": "node dist/lib/zones.js --clean",
    "clean:products": "node dist/lib/products.js --clean",
    "clean:taxCategories": "node dist/lib/tax-categories.js --clean",
    "clean:types": "node dist/lib/types.js --clean"
  },
  "devDependencies": {
    "@commercetools/sdk-auth": "^3.0.7",
    "cli-progress": "^3.4.0",
    "core-js": "3",
    "csvtojson": "^2.0.10",
    "es6-promise": "^4.2.8",
    "eslint-plugin-import": "^2.20.0",
    "isomorphic-fetch": "^3.0.0",
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.7.6",
    "@commercetools/api-request-builder": "5.6.3",
    "@commercetools/sdk-client": "2.1.2",
    "@commercetools/sdk-middleware-auth": "6.1.4",
    "@commercetools/sdk-middleware-http": "6.0.11",
    "babel-eslint": "^10.0.3",
    "braces": "^3.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.3.1",
    "fstream": "^1.0.12",
    "nconf": "^0.11.4",
    "path": "0.12.7",
    "prettier": "^2.0.5",
    "rimraf": "3.0.2",
    "snazzy": "9.0.0"
  },
  "private": true
}
